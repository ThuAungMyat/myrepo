
@model List<SampleWeb.Models.tbInventory>
<table class="table">
    <thead>
        <tr>
            <th>
                Item Name
            </th>
            <th>
                Flow Type
            </th>
            <th>
                Quantity
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.ItemName
                </td>
                <td>
                   @item.FlowType
                </td>
                <td>
                   @item.Qty
                </td>
            </tr>
        }
    </tbody>
</table>